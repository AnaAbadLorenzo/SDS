<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class table="rolaccionfuncionalidad" name="Pojos.RolAccionFuncionalidad">
	<composite-id>
		<key-property name="idAccion" column="idAccion"/>
		<key-property name="idFuncionalidad" column="idFuncionalidad"/>
		<key-property name="idRol" column="idRol"/>
	
	</composite-id>

</class>

</hibernate-mapping>
